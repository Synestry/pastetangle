{
    "name": "pastetangle",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://synestry.github.io/pastetangle",
    "dependencies": {
        "@iota/converter": "^1.0.0-beta.30",
        "@iota/mam": "^0.7.3",
        "@types/node": "^14.14.6",
        "ace-builds": "^1.4.12",
        "clipboard": "^1.7.1",
        "preact": "^10.5.5",
        "prettier": "^2.1.2",
        "querystate": "^1.0.8",
        "react-ace": "^9.2.0",
        "zlib": "^1.0.5"
    },
    "devDependencies": {
        "@types/clipboard": "^2.0.1",
        "parcel-bundler": "^1.12.4",
        "push-dir": "^0.4.1",
        "typescript": "^4.0.5"
    },
    "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
    },
    "scripts": {
        "start": "parcel public/index.html",
        "build": "parcel build public/index.html --public-url ./",
        "push-gh-pages": "yarn build && push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "htmlWhitespaceSensitivity": "css",
        "insertPragma": false,
        "jsxBracketSameLine": false,
        "jsxSingleQuote": true,
        "printWidth": 140,
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "requirePragma": false,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "none",
        "useTabs": true,
        "vueIndentScriptAndStyle": false
    }
}